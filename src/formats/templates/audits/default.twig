<section>
	<h2>{{data[0].audit|capitalize}}</h2>
	{% for item in data %}
		{% if item.value is iterable %}
			<h3>{{item.label}}</h3>
			<ul>
			{% for value in item.value %}
				{% if value.name %}
					<li>{{value.name}} - {{value.count}}</li>
				{% endif %}

				{% if value.file %}
					<li>{{value.file}} - {{value.selector}}</li>
				{% endif %}
			{% endfor %}
			</ul>
		{% else %}
			<h3>{{item.label}}: {{item.value}}</h3>
		{% endif %}
	{% endfor %}
		{# {% for item in data %}
		<h2>{{item.label}}</h2>
		<ul>
		</ul>
		{% endfor %} #}
	{# {% else %}
		<h1>not iterable</h1>
		<h2>{{data.label}}: {{data.value}}</h2>
	{% endif %} #}
</section>